<ul class="jqueryFileTree" style="display: none;">
{% for el in result%}
    {% if el.isDir %}
        <li class="directory collapsed"><a href="#" rel="{{el.FileName}}">{{el.FileName}}</a></li>
    {% else %}
    <li class="file ext_{{el.extension}}"><a href="#" rel="{{el.FileName}}">{{el.FileName}}</a></li>
    {% endif %}
{% endfor%} 
</ul>